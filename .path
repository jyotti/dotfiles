# /usr/local/bin を最優先に
export PATH=/usr/local/bin:$PATH

# GVM
if [ -f "$HOME/.gvm/bin/gvm-init.sh" ] ; then
    . "$HOME/.gvm/bin/gvm-init.sh"
fi

# rbenv
if _type rbenv; then
    eval "$(rbenv init -)"
fi

# pyenv
if _type pyenv; then
    eval "$(pyenv init -)"
fi

# jenv
if _type jenv ; then
    eval "$(jenv init -)"
    export JENV_ROOT="/usr/local/opt/jenv"
fi
